# Frontend environment configuration for Ocean Notes (React)

# Primary backend base URL. If set, the app uses this API for CRUD.
# Example: https://api.example.com
REACT_APP_API_BASE=

# Secondary/legacy backend URL key. Used only if REACT_APP_API_BASE is not set.
REACT_APP_BACKEND_URL=

# Optional additional variables (not required for basic usage)
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
